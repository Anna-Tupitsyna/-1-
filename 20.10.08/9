#include <bits/stdc++.h>
#include <iostream>
using namespace std;
bool isPrime(int n) {
  if (n<=3) {
    return true;
  }
  if (n%2==0 || n%3==0) {
    return false;
  }
  for (int i=5; i*i<=n; i=i+6) {
    if (n%i==0 || n%(i+2)==0) {
      return false;
    }
  }
  return true;
}
void printPrime(int n) {
  int k=0;
  while (k<=n) {
    for (int i=2; i<=2*n; i++) {
      if (isPrime(i)) {
        cout << i << " ";
      }
      k=k+1;
    }
  }
}
int main() {
  int n;
  cin >> n;
  printPrime(n);
}
