include <iostream>
using namespace std;                   
int X(int m, int n) 
{
  if (m==0) {
    return (n+1);
  }
  else if ((m>0) & (n==0)) {
    return X(m-1, 1);
  }
  else if ((m>0) & (n>0)) 
    return X(m-1, X(m, n-1));
}
int main () {
  int m, n;
  cin >> m;
  cin >> n;
  cout << X(m, n);
  return 0;
}
