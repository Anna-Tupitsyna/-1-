#include <iostream>
#include <vector>
#include "stdio.h"
#include "math.h"
#include "iostream"
using namespace std;
int main() {
    int a;
    cin >> a;
    vector <int> n(a);
    for (int i=0; i<a; i++) {
        cin >> n[i];
    }
    int k;
    cin >> k;
    int s=0;
    int i=0;
    while (i<a) {
        if (n[i]<=k) {
            int j=i;
            while (j<=a) {
                int r=0;
                if (r+n[j]<=k) {
                    r=r+n[j];
                    s=s+1;
                }
            }
        }
        else {
            i=i+1;
        }
    }
    cout << s;
    return 0;
}
