#include <iostream>
using namespace std;
int main() {
  int n, s=0;
  cin >> n;
  int i=1;
  while (i<=n) {
    int k;
    cin >> k;
    s=s+k*k*k;
    i=i+1;
  }
  cout << (s/n);
  return 0;
}


#include <iostream>
using namespace std;
int main() {
	int n, k, m, s=1, t=1, r=1;
	cin >> n;
	cin >> k;
	m=n-k;
	for (int i=1; i<=n; i++) {
		s=s*i;
	}
	for (int i=1; i<=k; i++) {
		t=t*i;
	}
	for (int i=1; i<=m; i++) {
		r=r*i;
	}
	cout << (s/(t*r));
	return 0;
}

#include <iostream>
using namespace std;                   
int X(int m, int n) 
{
  if (m==0) {
    return (n+1);
  }
  else if ((m>0) & (n==0)) {
    return X(m-1, 1);
  }
  else if ((m>0) & (n>0)) 
    return X(m-1, X(m, n-1));
}
int main () {
  int m, n;
  cin >> m;
  cin >> n;
  cout << X(m, n);
  return 0;
}
