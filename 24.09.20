#include <iostream>
#include<vector>
#include<stdlib.h>

using namespace std;
int main() {
  int n;
  cin >> n;
  int s[n];
  int x=0;
  for(int k=0; k<n; k++) {
    cin >> s[k];
  }
  for(int i=0; i<n; i++) {
     for(int j=n-1; j>i; j--) {
        if(s[j-1]>s[j]) {
          swap(s[j-1], s[j]); x=x+1;
        }
     }
  }
  cout << x;
  return 0;
}


